<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fast.dev.platform.android"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.READ_LOGS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:name="fast.dev.platform.android.app.MyApplication"
        android:theme="@style/MyTheme.LoadingPage" >
        <activity
            android:name="fast.dev.platform.android.activity.LoadingActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="fast.dev.platform.android.activity.MainActivity"></activity>
        <activity android:name="fast.dev.platform.android.activity.BootActivity"></activity>
        <activity android:name="fast.dev.platform.android.activity.account.LoginActivity"></activity>
        <activity android:name="fast.dev.platform.android.activity.account.ProfileActivity"></activity>
        <activity android:name="fast.dev.platform.android.activity.account.RegisterActivity"></activity>
        <activity android:name="fast.dev.platform.android.activity.account.ModifyPasswordActivity"></activity>
    </application>

</manifest>